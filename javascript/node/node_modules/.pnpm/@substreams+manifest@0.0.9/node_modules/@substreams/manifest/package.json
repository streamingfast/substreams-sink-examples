  },
  "dependencies": {
    "@bufbuild/buf": "^1.25.0",
    "@bufbuild/protobuf": "^1.2.1",
    "@effect/schema": "^0.32.0",
    "@substreams/core": "^0.1.19",
    "yaml": "^2.3.1"
  },
  "files": [
    "system.pb",
    "dist/**",
    "!dist/**/.tsbuildinfo",
    "src/**/*.ts",
    "!src/**/*.test.ts",
    "src/**/*.pb"
  ]
